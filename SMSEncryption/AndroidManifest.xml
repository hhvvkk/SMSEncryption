<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.cos730.smsencryption"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.cos730.smsencryption.ContactListActivity"
            android:label="@string/Contacts" >
        </activity>
        <activity
            android:name="com.cos730.smsencryption.ContactDetailActivity"
            android:label="@string/title_contact_detail"
            android:parentActivityName="com.cos730.smsencryption.ContactListActivity" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.cos730.smsencryption.ContactListActivity" />
        </activity>
        <activity
            android:name="com.cos730.user.LoginActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.cos730.user.AddUserActivity"
            android:label="@string/title_activity_add_user" >
        </activity>
        <activity
            android:name="com.cos730.contact.AddContactActivity"
            android:label="@string/title_activity_add_contact" >
        </activity>
        <activity
            android:name="com.cos730.contact.EditContactActivity"
            android:label="@string/title_activity_edit_contact" >
        </activity>
    </application>

</manifest>
